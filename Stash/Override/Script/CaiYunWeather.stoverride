name: 彩云天气SVIP
desc: 彩云天气会员解锁

http:
  mitm:
    - "*.caiyunapp.com"
    - "*.cyapi.com"
    - "*.cyapi.cn"
    - "*.caiyunapp.cn"
  rewrite:
    - ^https:\/\/ad\.caiyunapp\.com\/ - reject
  script:
    - match: ^http[s]?:\/\/biz\.(caiyunapp|cyapi)\.(com|cn)\/v2\/user.*$
      name: CaiYunWeather
      type: response
      require-body: true
      timeout: 10

script-providers:
  CaiYunWeather:
    url: https://raw.githubusercontent.com/WeiRen0/Scripts/main/cytq.js
    interval: 86400
